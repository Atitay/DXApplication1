@model GorgeShipping.Models.ViewModel.UserListViewModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
    <h2 class="text-info">New Phone Number</h2>
    <form method="post">
        <br />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <div class="col-12">

            <div class="form-group row">
                <div class="col-2">
                    <label asp-for="TelephoneNumbers.TelNumber"></label>
                </div>
                <div class="col-3">
                    @(Html.DevExtreme().TextBoxFor(m => m.TelephoneNumbers.TelNumber).Mask("000-000-0000"))
                </div>
            </div>

          
            <br />
            <div class="form-group">
                <input type="submit" class="btn btn-outline-info" value="Create" />
                <a asp-controller="Home" asp-action="Index"  class="btn btn-outline-success">Back to List</a>
            </div>
        </div>
    </form>


    @section Scripts{
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
</body>
</html>
