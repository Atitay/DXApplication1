@model GorgeShipping.Models.TelNo

<div asp-validation-summary="ModelOnly" class="text-danger"></div>

<div class="form-group rounded">
    <div class="col">
        @*<label asp-for="TelNumber"></label>*@
    </div>
    <div class="col">
        @(Html.DevExtreme().TextBoxFor(m => m.TelNumber).Mask("000-000-0000").Width(200).Height(30).ReadOnly(true))
    </div>
    <span asp-validation-for="TelNumber" class="text-danger"></span>
    <br />
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
